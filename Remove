import java.util.*;
public class Remove {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		int arr[]=new int[n];
		for(int i=0;i<n;i++){
			arr[i]=sc.nextInt();
		}
		int key = sc.nextInt();
		sc.close();
		int m=0;
		for(int i=0;i<n;i++)
		{
			if(arr[i]==key)
				m++;
		}
		if(m==0)
		{
			System.out.println(arr.length);
		}
		else
			System.out.println(arr.length-m);
	}

}
